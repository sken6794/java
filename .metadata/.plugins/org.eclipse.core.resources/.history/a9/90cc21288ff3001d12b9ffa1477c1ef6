package quiz01;

import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;

public class Main {
	public static void main(String[] args) {
		
		String[] str_list = {"u","u","l","r"};
		String[] result = {};
		
		List<String> list = new ArrayList<>();
		for (String string : str_list) {
			list.add(string);
		}
		// l 이 먼저 나왔을 때 l 직전까지 출력
		// r 이 먼저 나왔을 때 r 이후 출력
		int l = list.indexOf("l");
		int r = list.indexOf("r");
		 
		if(l>r && r!=-1) { //r이 먼저 나왔을 때
			for (int i = r+1; i < list.size(); i++) {
				int j = 0;
				result[j] = list.get(j+i);
				j++;
			}
		} else if(l<r && l!=-1) { // l 이 먼저나왔을 때 
			for (int i = 0; i < l; i++) {
				result[i] = list.get(i);
			}
		}
		
		
		
		
		
		
		
		
		
		
	}
	
	
}
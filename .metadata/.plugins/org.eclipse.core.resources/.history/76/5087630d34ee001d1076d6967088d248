		String pattern = "\\d{6}-\\d{7}";
		String pattern2 = "[A-Z]\\d*";
		String pattern3 = "\\(+[가-힣]*\\)";
		String pattern4 = "\\d*,+\\d*원";
		
		Matcher m = Pattern.compile(pattern).matcher(str);
		List<String> list = Arrays.asList(str,str2);
		
		
		
		
		
		
		
	}

package quiz01;

public class Main {
	public static void main(String[] args) {
		
		String[] id_pw = {"meosseugi","1234"};
		String[][] db = {
				{"rardss", "123"},{"yyoom", "1234"},
				{"meosseugi", "1234"}
		};
		
		String answer = "";
		for (int i = 0; i < db.length; i++) {
			for (int j = 0; j < db[i].length; j++) {
				if(id_pw[0].equals(db[i][0]) && id_pw[1].equals(db[i][1])) {
					answer = "login";
				}
			}
		}
		
		

	}
}
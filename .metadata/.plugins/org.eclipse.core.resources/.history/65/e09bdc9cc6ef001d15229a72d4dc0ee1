package quiz01;

import java.util.ArrayList;
import java.util.List;

public class Main {
	public static void main(String[] args) {
		
		List<Integer> d = new ArrayList<>();
		d.add(1);
		d.add(1);
		d.add(1);
		d.add(1);
		d.add(1);
		d.add(1);
		d.add(1);
	 	

	}
	
	public static int fac(int i) {
		
		if(i==1) return 1;
		
		
		return i*fac(i-1);
	};
}
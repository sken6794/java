package quiz01;

import java.util.Arrays;

public class Main {
	public static void main(String[] args) {
		
		String[] str_list = {"l"};
		String str = "";
		String[] result;
		for (String string : str_list) {
			str += string;
		}
		
		boolean f = false;
		//r이 뒤에나오면 f , 먼저나오면 t 
		if(str.indexOf("l")<str.indexOf("r")) f = false;
		else f = true;
		
		if(f) { //r이 먼저
			result = new String[str_list.length-str.indexOf("r")];
			for (int i = str.indexOf("r"); i < str_list.length; i++) {
				int j = 0;
				if(str.indexOf("l")==0) {
					result = new String[0];
					break;
				}
				result[j] = str_list[i+1];
				j++;
			}
		} else {
			
			result = new String[str.indexOf("l")];
			for (int i = 0; i < result.length; i++) {
				if(str.indexOf("l")==0) {
					result = new String[0];
					break;
				}
				result[i] = str_list[i];
			}
		}
		
		System.out.println(Arrays.toString(result));
		
		
		
	}
	
	
}